<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="426.0" prefWidth="339.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.poli.paradigmas.tc.proyecto.GUI.controller.MenuVehiculosController">
   <children>
      <Button fx:id="btnAgregarVehiculo" alignment="CENTER" layoutX="33.0" layoutY="114.0" maxWidth="Infinity" onAction="#agregarNuevoVehiculo" prefHeight="70.0" prefWidth="125.0" style="-fx-background-color: #deecf4;" text="Agregar Nuevo Vehículo" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Times New Roman Bold" size="12.0" />
         </font>
         <graphic>
            <ImageView fitHeight="37.0" fitWidth="42.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../imagenes/queue_icon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="btnBuscarVehiculo" layoutX="33.0" layoutY="195.0" maxWidth="Infinity" onAction="#buscarVehiculoPorPlaca" prefHeight="70.0" prefWidth="125.0" style="-fx-background-color: #deecf4;" text="Buscar Vehículo por Placa" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Times New Roman Bold" size="12.0" />
         </font>
         <graphic>
            <ImageView fitHeight="41.0" fitWidth="49.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../imagenes/search_zoom_icon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="btnActualizarDisponibilidad" layoutX="182.0" layoutY="195.0" maxWidth="Infinity" onAction="#actualizarDisponibilidadVehiculo" prefHeight="70.0" prefWidth="125.0" style="-fx-background-color: #deecf4;" text="Actualizar Disponibilidad" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Times New Roman Bold" size="12.0" />
         </font>
         <graphic>
            <ImageView fitHeight="30.0" fitWidth="56.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../imagenes/update_arrow_icon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="btnEliminarVehiculo" layoutX="182.0" layoutY="114.0" maxWidth="Infinity" onAction="#eliminarVehiculoPorPlaca" prefHeight="70.0" prefWidth="125.0" style="-fx-background-color: #deecf4;" text="Eliminar Vehículo por Placa" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Times New Roman Bold" size="12.0" />
         </font>
         <graphic>
            <ImageView fitHeight="35.0" fitWidth="48.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../imagenes/delete_remove_icon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button fx:id="btnMostrarVehiculos" layoutX="107.0" layoutY="277.0" maxWidth="Infinity" onAction="#mostrarTodosLosVehiculos" prefHeight="70.0" prefWidth="125.0" style="-fx-background-color: #deecf4;" text="Mostrar Todos los Vehículos" textAlignment="CENTER" wrapText="true">
         <font>
            <Font name="Times New Roman Bold" size="12.0" />
         </font>
         <graphic>
            <ImageView fitHeight="40.0" fitWidth="44.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../imagenes/view_icon.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Button layoutX="182.0" layoutY="389.0" mnemonicParsing="false" onAction="#volverMenuPrincipal" text="Volver Al Menu Principal">
         <font>
            <Font name="Times New Roman Bold" size="12.0" />
         </font>
      </Button>
      <ImageView fitHeight="58.0" fitWidth="96.0" layoutX="213.0" layoutY="27.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../imagenes/poli.png" />
         </image>
      </ImageView>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="33.0" layoutY="23.0" prefHeight="65.0" prefWidth="182.0" text="Menu Gestor Vehiculos" wrapText="true">
         <font>
            <Font name="Times New Roman Bold" size="25.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
